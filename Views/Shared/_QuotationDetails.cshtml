@model BizBulkApp.Models.Quotation

<div class="quotation-result">
    <h4>Quotation Details</h4>
    <dl class="row">
        <dt class="col-sm-2">Track Number</dt>
        <dd class="col-sm-10">@Model.quotationTrackNumber</dd>
        <dt class="col-sm-2">Buyer</dt>
        <dd class="col-sm-10">@Model.Buyer?.Name</dd>
        <dt class="col-sm-2">Created Date</dt>
        <dd class="col-sm-10">@Model.CreatedDate</dd>
        <dt class="col-sm-2">Status</dt>
        <dd class="col-sm-10">@Model.Status</dd>
    </dl>
</div>
